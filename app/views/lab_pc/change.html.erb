        <div class="container">
            <div class="header">
              <ul class="nav nav-pills pull-right">
                <li><%= link_to 'Home',home_index_path %></li>
                <li><a href="borrow.html">Borrow</a></li>
                <li><%= link_to 'Resources', resources_path %></li>
                <li><%= link_to 'User', edit_user_registration_path %></li>
                <li><%= link_to "ログアウト", destroy_user_session_path, method: :delete %></li>
                <% if current_user.status=="manager" %>
                <li><li class="active"><a href="index.html">CHINO</a></li>
                <% end %>
              </ul>
              <% if current_user.status == "manager" %>
                  <h3 class="text-muted">Manager</h3>
                  <% else %>
                  <h3 class="text-muted">User</h3>
                  <% end %>
            </div>
              <%= form_for @lab_pc, :url=> {:action => 'change_update'} do |f| %>
              <div class="field">
              <%= f.label :machine_name %>
              <%= f.text_field :machine_name, autofocus: true %>
            </div>
              <div class="field">
              <%= f.label :cpu %>
              <%= f.text_field :cpu, autofocus: true %>
            </div>
              <div class="field">
              <%= f.label :memory %>
              <%= f.text_field :memory, autofocus: true %>
            </div>
              <div class="field">
              <%= f.label :hdd %>
              <%= f.number_field :hdd, autofocus: true %><p>GB</p>
            </div>
              <div class="field">
              <%= f.label :return_status %><br />
              <%= f.select :return_status,["unapplied","applying","returned"], autofocus: true %>
            </div>
              <%= f.submit %>
              <% end %>
              <div>
